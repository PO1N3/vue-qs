{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "tsconfig": "tsconfig.json",
  "plugin": ["typedoc-plugin-markdown"],
  "out": "docs/api",
  "readme": "none",
  "includeVersion": true,
  "hideGenerator": true,
  "categorizeByGroup": false,
  "sort": ["source-order"],
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true
}
